OLDSPOOLCLIB_DIR = $(SDIR)/libs/spool/classic

OLDSPOOLCLIB = libspoolc_old$(LIBEXT)

OLDSPOOLCLIB_SRC = \
         sge_spooling_classic.c \
         read_write_sharetree.c \
         read_write_complex.c \
         read_list.c \
         read_object.c \
         read_write_advance_reservation.c \
         read_write_cal.c \
         read_write_ckpt.c \
         read_write_cqueue.c \
         read_write_host.c \
         read_write_host_group.c \
         read_write_centry.c \
         read_write_job.c \
         read_write_resource_quota.c \
         read_write_manop.c \
         read_write_pe.c \
         read_write_qinstance.c \
         read_write_ume.c \
         read_write_userprj.c \
         read_write_userset.c \
         rw_configuration.c \
         sched_conf.c
         

OLDSPOOLCLIB_OBJS = $(OLDSPOOLCLIB_SRC:.c=.o)

###############################################################################
# global rules: all, clean, depend, messages
###############################################################################
spoolclib_old: $(OLDSPOOLCLIB)

spoolclib_old_clean:
		$(RM) $(OLDSPOOLCLIB_OBJS) libspoolc_old.a libspoolc_old$(SHAREDEXT)

spoolclib_old_depend:
		$(DEPEND) -f$(OLDSPOOLCLIB_DIR)/spoolc_old_dependencies $(OLDSPOOLCLIB_DIR)/*.c

spoolclib_old_messages:
	@echo Creating $@
	@for i in `ls $(OLDSPOOLCLIB_DIR)/*.c`; do \
	   $(CC) $(CFLAGS) -DEXTRACT_MESSAGES -E $$i | $(XGETTEXT) - ;\
	done

###############################################################################
# rules to build library
###############################################################################
libspoolc_old.a:	$(OLDSPOOLCLIB_OBJS) 
	$(AR) $(ARFLAGS) libspoolc_old.a $(OLDSPOOLCLIB_OBJS)
	$(RANLIB) libspoolc_old.a

libspoolc_old$(SHAREDEXT): $(OLDSPOOLCLIB_OBJS) $(SPOOLLIB) $(SCHEDLIB) $(SGEOBJLIB) $(SGEOBJDLIB) $(COMMLIB) $(COMMLISTSLIB) $(CULLLIB) $(UTILIB) $(RMONLIB) $(LCKLIB)
	$(SHAREDLD) $(SHARED_LFLAGS) -o libspoolc_old$(SHAREDEXT) $(OLDSPOOLCLIB_OBJS) -lspool -lsched -lsgeobj -lsgeobjd -lcull -lcomm -lcommlists -luti -lrmon -llck $(LIBS) -lc

###############################################################################
# include dependencies for object files generated by sge_depend
###############################################################################
include $(OLDSPOOLCLIB_DIR)/spoolc_old_dependencies

###############################################################################
# rules to build object codes
###############################################################################
sge_spooling_classic.o: $(OLDSPOOLCLIB_DIR)/sge_spooling_classic.c 
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(OLDSPOOLCLIB_DIR)/sge_spooling_classic.c

read_write_sharetree.o: $(OLDSPOOLCLIB_DIR)/read_write_sharetree.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(OLDSPOOLCLIB_DIR)/read_write_sharetree.c

read_write_complex.o: $(OLDSPOOLCLIB_DIR)/read_write_complex.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(OLDSPOOLCLIB_DIR)/read_write_complex.c

read_object.o:   $(OLDSPOOLCLIB_DIR)/read_object.c
		$(CC) $(CFLAGS) $(MEMCFLAG) $(SHARED_CFLAGS) -c $(OLDSPOOLCLIB_DIR)/read_object.c

read_write_cal.o:	$(OLDSPOOLCLIB_DIR)/read_write_cal.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(OLDSPOOLCLIB_DIR)/read_write_cal.c

