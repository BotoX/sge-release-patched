SPOOLCLIB_DIR = $(SDIR)/libs/spool/classic

SPOOLCLIB = libspoolc$(SPOOLLIBEXT)

SPOOLCLIB_SRC = \
         sge_spooling_classic.c \
         read_write_sharetree.c \
         read_write_complex.c \
         read_list.c \
         read_object.c \
         read_write_cal.c \
         read_write_ckpt.c \
         read_write_cqueue.c \
         read_write_host.c \
         read_write_host_group.c \
         read_write_centry.c \
         read_write_job.c \
         read_write_manop.c \
         read_write_pe.c \
         read_write_qinstance.c \
         read_write_ume.c \
         read_write_userprj.c \
         read_write_userset.c \
         rw_configuration.c \
         sched_conf.c
         

SPOOLCLIB_OBJS = $(SPOOLCLIB_SRC:.c=.o)

###############################################################################
# global rules: all, clean, depend, messages
###############################################################################
spoolclib: $(SPOOLCLIB)

spoolclib_clean:
		$(RM) $(SPOOLCLIB_OBJS) libspoolc.a libspoolc$(SHAREDEXT)

spoolclib_depend:
		$(DEPEND) -f$(SPOOLCLIB_DIR)/spoolc_dependencies $(SPOOLCLIB_DIR)/*.c

spoolclib_messages:
	@echo Creating $@
	@for i in `ls $(SPOOLCLIB_DIR)/*.c`; do \
	   $(CC) $(CFLAGS) -DEXTRACT_MESSAGES -E $$i | $(XGETTEXT) - ;\
	done

###############################################################################
# rules to build library
###############################################################################
libspoolc.a:	$(SPOOLCLIB_OBJS) 
	$(AR) $(ARFLAGS) libspoolc.a $(SPOOLCLIB_OBJS)
	$(RANLIB) libspoolc.a

libspoolc$(SHAREDEXT): $(SPOOLCLIB_OBJS) $(SPOOLLIB) $(SCHEDLIB) $(SGEOBJLIB) $(SGEOBJDLIB) $(COMMLIB) $(COMMLISTSLIB) $(CULLLIB) $(UTILIB) $(RMONLIB) $(LCKLIB)
	$(SHAREDLD) $(SHARED_LFLAGS) -o libspoolc$(SHAREDEXT) $(SPOOLCLIB_OBJS) -lspool -lsched -lsgeobj -lsgeobjd -lcull -lcomm -lcommlists -luti -lrmon -llck $(LIBS) -lc

###############################################################################
# include dependencies for object files generated by sge_depend
###############################################################################
include $(SPOOLCLIB_DIR)/spoolc_dependencies

###############################################################################
# rules to build object codes
###############################################################################
sge_spooling_classic.o: $(SPOOLCLIB_DIR)/sge_spooling_classic.c 
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/sge_spooling_classic.c

read_write_sharetree.o: $(SPOOLCLIB_DIR)/read_write_sharetree.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_sharetree.c

read_write_complex.o: $(SPOOLCLIB_DIR)/read_write_complex.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_complex.c

read_object.o:   $(SPOOLCLIB_DIR)/read_object.c
		$(CC) $(CFLAGS) $(MEMCFLAG) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_object.c

read_write_cal.o:	$(SPOOLCLIB_DIR)/read_write_cal.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_cal.c

read_write_centry.o:	$(SPOOLCLIB_DIR)/read_write_centry.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_centry.c

read_write_ckpt.o:	$(SPOOLCLIB_DIR)/read_write_ckpt.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_ckpt.c

read_write_cqueue.o:	$(SPOOLCLIB_DIR)/read_write_cqueue.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_cqueue.c

read_write_host.o   : $(SPOOLCLIB_DIR)/read_write_host.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_host.c

read_write_host_group.o:	$(SPOOLCLIB_DIR)/read_write_host_group.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_host_group.c

read_write_job.o:	$(SPOOLCLIB_DIR)/read_write_job.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_job.c

read_write_manop.o:	$(SPOOLCLIB_DIR)/read_write_manop.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_manop.c

read_write_pe.o:	$(SPOOLCLIB_DIR)/read_write_pe.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_pe.c

read_write_qinstance.o:	$(SPOOLCLIB_DIR)/read_write_qinstance.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_qinstance.c

read_write_ume.o:	$(SPOOLCLIB_DIR)/read_write_ume.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_ume.c

read_write_userprj.o:	$(SPOOLCLIB_DIR)/read_write_userprj.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_userprj.c

read_write_userset.o:	$(SPOOLCLIB_DIR)/read_write_userset.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_write_userset.c

rw_configuration.o:  $(SPOOLCLIB_DIR)/rw_configuration.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/rw_configuration.c

sched_conf.o   : $(SPOOLCLIB_DIR)/sched_conf.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/sched_conf.c

read_list.o   : $(SPOOLCLIB_DIR)/read_list.c
		$(CC) $(CFLAGS) $(SHARED_CFLAGS) -c $(SPOOLCLIB_DIR)/read_list.c

